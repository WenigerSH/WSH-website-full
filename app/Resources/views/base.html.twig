<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    
    <title>
        {% block title %}
            at Weniger Software House
        {% endblock %}
    </title>
    {% block head_bottom %}
        <meta name="author" content="Weniger Software House">
    {% endblock %}
    {% block stylesheets %}

    <!--[if !lte IE 6]><!-->
        {% stylesheets 'bundles/wshwebsite/css/*' filter='cssrewrite' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

        <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Open+Sans:400,600,300,800,700,400italic|PT+Serif:400,400italic" />
    <!--<![endif]-->

    <!--[if lte IE 6]>
        <link rel="stylesheet" href="//universal-ie6-css.googlecode.com/files/ie6.1.1.css" media="screen, projection">
    <![endif]-->
    {% endblock %}
    <!-- HTML5 Shiv + detect touch events -->
    <script src="/bundles/wshwebsite/js/modernizr.custom.js"></script>

    <!-- HTML5 video player -->
    <script src="/bundles/wshwebsite/js/video.min.js"></script>
    <script>_V_.options.flash.swf = 'http://localhost/smartstart/js/video-js.swf';</script>
<!-- Shared on MafiaShare.net  --><!-- Shared on MafiaShare.net  --></head>
<body>

<header id="header" class="container clearfix">

    <a href="{{ path('homepage') }}" id="logo" title="Go to homepage">
        <img src="{{ asset('bundles/wshwebsite/img/logo.png') }}" alt="Weniger Software House logo">
    </a>

    <nav id="main-nav">
        {{ knp_menu_render('WshWebsiteBundle:Builder:mainMenu') }}
    </nav><!-- end #main-nav -->
    
</header><!-- end #header -->
<section id="content">
    <div class="container clearfix">
        {% block body %}
        {% endblock %}
    </div>
</section><!-- end #content -->


<footer id="footer" class="clearfix">

    <div class="container">

        <div class="three-fourth footer">

            <nav id="footer-nav" class="clearfix">
                {{ knp_menu_render('WshWebsiteBundle:Builder:mainMenu') }}
            </nav><!-- end #footer-nav -->

            <ul class="contact-info">
                <li class="address">Lęborska 3b, 80-386 Gdańsk, Poland</li>
                <li class="phone">+48 58 731 65 04</li>
                <li class="email"><a href="mailto:contact@wenigersh.com">contact@wenigersh.com</a></li>
            </ul><!-- end .contact-info -->
            
        </div><!-- end .three-fourth -->

        <div class="one-fourth stay-connected">

            <span class="title">Stay connected</span>

            <ul class="social-links">
                <li class="facebook"><a href="http://www.facebook.com/WenigerSH">Facebook</a></li>
                <li class="github"><a href="https://github.com/WenigerSH">GitHub</a></li>
                {# <li class="twitter"><a href="#">Twitter</a></li>
                <li class="digg"><a href="#">Digg</a></li>
                <li class="vimeo"><a href="#">Vimeo</a></li>
                <li class="youtube"><a href="#">YouTube</a></li>
                <li class="skype"><a href="#">Skype</a></li> #}
            </ul><!-- end .social-links -->

        </div><!-- end .one-fourth.last -->

        <div class="eu-projects">
            <a href="/eu-projects"><img src="{{ asset('bundles/wshwebsite/img/eu.png') }}" alt="" /></a>
        </div>
        
    </div><!-- end .container -->

</footer><!-- end #footer -->

<footer id="footer-bottom" class="clearfix">

    <div class="container">

        <ul>
            <li>Weniger Software House &copy; {{ "now"|date("Y") }} | </li>
            <li style="margin-right: 10px;">We use cookies on our website.</li>
            <li><a href="/eu-projects">EU projects (realizowane projekty EU)</a></li>
            <li><a href="/zapytania-ofertowe">Zapytania ofertowe</a></li>
        </ul>

    </div><!-- end .container -->

</footer><!-- end #footer-bottom -->
{% block javascripts %}
<!--[if !lte IE 6]><!-->
