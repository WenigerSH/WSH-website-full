    <script src="/bundles/wshwebsite/js/jquery.fancybox.pack.js"></script>
    <script src="/bundles/wshwebsite/js/jquery.smartStartSlider.min.js"></script>
    <script src="/bundles/wshwebsite/js/jquery.jcarousel.min.js"></script>
    <script src="/bundles/wshwebsite/js/jquery.cycle.all.min.js"></script>
    <script src="/bundles/wshwebsite/js/jquery.isotope.min.js"></script>
    <script src="/bundles/wshwebsite/js/audioplayerv1.min.js"></script>
    <script src="//maps.google.com/maps/api/js?sensor=false"></script>
    <script src="/bundles/wshwebsite/js/jquery.gmap.min.js"></script>
    <script src="/bundles/wshwebsite/js/jquery.touchSwipe.min.js"></script>
    <script src="/bundles/wshwebsite/js/custom.js"></script>
<!--<![endif]-->
{% endblock %}
